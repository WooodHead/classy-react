{
    "name": "classy-react",
    "displayName": "Classy React",
    "description": "Helps convert from a react function to a react class, and a stateless react class to a react function.",
    "version": "0.0.1",
    "publisher": "aaronYoung",
    "engines": {
        "vscode": "^1.21.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/brainomite/classy-react.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.convertToFunctions",
        "onCommand:extension.convertToClass"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.convertToFunctions",
                "title": "Classy React: Convert To Functions"
            },
            {
                "command": "extension.convertToClass",
                "title": "Classy React: Convert To Class"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "prepush": "npm test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "babel-eslint": "^8.2.2",
        "chai": "^4.1.2",
        "eslint": "^4.11.0",
        "eslint-config-fullstack": "^3.0.0",
        "eslint-plugin-react": "^7.7.0",
        "husky": "^0.14.3",
        "react": "^16.2.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "babel": "^6.23.0",
        "babel-generator": "^6.26.1",
        "babel-template": "^6.26.0",
        "babel-traverse": "^6.26.0",
        "babel-types": "^6.26.0",
        "babylon": "^6.18.0"
    }
}
